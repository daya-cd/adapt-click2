

<div class="clicktwo-inner component-inner" role="region" aria-label="{{_globals._components._clicktwo.ariaRegion}}">
  {{> component this}}
  <div class="clicktwo-widget component-widget {{#if _flag}}clicktwo-{{_flag}}{{/if}}">

   {{#if invert}}

   <div class="clicktwo-tabViewContainer" style="height: 300px;">
    {{#each _items}}
    <div class="clicktwo-tabItem">
      <div class="clicktwo-tabView-title">{{{title}}}</div>
      <div class="clicktwo-tabView-body">{{{body}}}</div>
    </div>
    {{/each}}
  </div>


  <ul  class="clicktwo-navContainer">
{{#each _items}}
   
    <li class="clicktwo-indicator"><img src="{{_graphic.src}}" class="clicktwo-indicator-img" aria-label="{{_graphic.alt}}" ></li>
     {{/each}}
  </ul>



  {{else}}
  <ul class="clicktwo-navContainer">
    {{#each _items}}
    <li class="clicktwo-indicator">
      <img src="{{_graphic.src}}" class="clicktwo-indicator-img" aria-label="{{_graphic.alt}}" tabindex="0">
    </li>
    {{/each}}
  </ul>


  <div class="clicktwo-tabViewContainer">
    {{#each _items}}
    <div class="clicktwo-tabItem">
      <div class="clicktwo-tabView-title">{{{title}}}</div>
      <div class="clicktwo-tabView-body">{{{body}}}</div>
    </div>
    {{/each}}
  </div>
  {{/if}}



  <div class="clicktwo-popup display-none">
    <div class="clicktwo-popup-inner">
      <div class="clicktwo-popup-toolbar">
        <div class="clicktwo-popup-toolbar-inner clearfix">
          {{#each _items}}
          <div class="clicktwo-popup-toolbar-title">
            <h5 class="clicktwo-popup-toolbar-inner">
              {{{title}}}
            </h5>
          </div>
          {{/each}}
          <a href="#" class="clicktwo-popup-close">
            <div class="icon icon-cross"></div>
          </a>
        </div>
      </div>
      {{#each _items}}
      <div class="clicktwo-popup-body">
        <div class="clicktwo-popup-body-inner">
          {{{body}}}
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
</div>
